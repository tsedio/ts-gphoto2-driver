{
  "name": "@tsed/examples-typescript",
  "version": "3.1.0",
  "private": true,
  "dependencies": {
    "@tsed/gphoto2-driver": "3.1.0"
  },
  "devDependencies": {
    "cli-table3": "0.6.0"
  },
  "scripts": {
    "start:abilities": "ts-node -r tsconfig-paths/register src/abilities-list.ts",
    "start:camera": "ts-node -r tsconfig-paths/register src/camera.ts",
    "start:camera:abilities": "ts-node -r tsconfig-paths/register src/camera-abilities.ts",
    "start:camera:autoselect": "ts-node -r tsconfig-paths/register src/camera-autoselect.ts",
    "start:camera:info": "ts-node -r tsconfig-paths/register src/camera-info.ts",
